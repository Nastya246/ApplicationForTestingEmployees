@model IEnumerable<WebApplicationForTest.Models.Результат_теста>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutScriptNotBack.cshtml";
}
<small><a href="~/Home/Index">Сменить пользователя</a></small>
<h2>Ваши результаты:</h2> <br />
                          <table class="table">

                              <tr>
                                  <td>
                                      <br /><br /><p><big>Оценка:</big> @ViewBag.ResultProcent %</p><br /><br />
                                  </td>
                              </tr>
                              <tr>
                                  <td>
                                      <form method="post" action="~/Разделы/Index">
                                          <br /><input type="submit" value="Выбрать другой тест" />
                                          <input type="hidden" value=@ViewBag.Id_user name="id_user" />
                                          <input type="hidden" value=@ViewBag.Data name="Data" />
                                      </form>
                                  </td>
                              </tr>
                              <tr>
                                  <td>
                                      <br />
                                  </td>
                              </tr>
                              @foreach (string result1 in ViewBag.QuestionAnswer)
                              {
                                  foreach (var part in result1.Split(new string[] { "\n" }, StringSplitOptions.RemoveEmptyEntries))
                                  {
                                      <tr>
                                          <td>
                                              @part
                                          </td>
                                      </tr>

                                  }
                                  <tr>
                                      <td>
                                          <br />
                                      </td>
                                  </tr>
                              }


                          </table>


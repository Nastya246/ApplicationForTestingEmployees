@model IEnumerable<WebApplicationForTest.Models.Подразделение>

@{
  
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2><big>Список подразделений</big><br /><br /><br /></h2>

<p>
    <big>  <big> @Html.ActionLink("Добавить подразделение", "Create")  </big> </big><br /><br />
</p>
<table class="table">


    @foreach (var item in Model)
    {
        <tr>
            <td>
                <big>   <big> @Html.DisplayFor(modelItem => item.Название_подразделения)</big></big><br /><br />
            </td>
        </tr>
        foreach (var itemD in item.Должность)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => itemD.Название_должности)
                </td>
            </tr>

        }
        <tr>
            <td>
                <br /><br />
                @Html.ActionLink("Изменить", "Edit", new { id = item.Id_подразделения }) |
                @Html.ActionLink("Детали", "Details", new { id = item.Id_подразделения }) |
                @Html.ActionLink("Удалить", "Delete", new { id = item.Id_подразделения })  <br /><br />
            </td>
        </tr>
    }

    
</table>
<form method="post" action="~/Home/Menu">
    <input type="hidden" value="admin" name="Login" />
    <input type="hidden" value="admin12345" name="Password" />
    <input type="submit" value="В меню" />

</form>